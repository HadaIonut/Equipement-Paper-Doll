<form>
    <div class="container">
        <div class="paperDollImage">
            {{#each itemTypesArray}}
                <div id="{{this}}" class="{{leftOrRight @index}}">
                    <h3>{{this}}</h3>
                    <div class="flexrow">
                        <div class="addBox"><i class="fas fa-plus-square"></i></div>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
    <button type="submit">Submit</button>
</form>
