<div>
    <div class="container">
        <div class="paperDollImage">
            {{#times 13}}
                <div class="{{leftOrRight this}}">
                    <select class="selector" name="{{this}}">
                        <option value="" disabled selected>Select your option</option>
                    </select>
                </div>
            {{/times}}
        </div>
        <div>
            {{#times 2}}
                <div>
                    <select class="selector" name="{{this}}">
                        <option value="" disabled selected>Select your option</option>
                    </select>
                </div>
            {{/times}}
        </div>
    </div>
</div>